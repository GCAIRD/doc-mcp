{
	"name": "gc-doc-mcp-monorepo",
	"version": "0.1.0",
	"private": true,
	"workspaces": [
		"src/shared",
		"src/embed",
		"src/mcp",
		"src/frontend",
		"benchmark"
	],
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"build": "npm run build --workspaces",
		"build:shared": "npm run build -w @gc-doc/shared",
		"build:embed": "npm run build -w @gc-doc/embed",
		"build:mcp": "npm run build -w @gc-doc/mcp",
		"dev": "npm run dev -w @gc-doc/mcp",
		"dev:frontend": "npm run dev -w doc-mcp-frontend",
		"embed": "npm run embed -w @gc-doc/embed",
		"typecheck": "npm run typecheck --workspaces",
		"test": "vitest"
	},
	"devDependencies": {
		"typescript": "^5.6.3",
		"vitest": "^2.1.4"
	}
}
