# === Product Selection (逗号分隔，支持多产品) ===
PRODUCT=spreadjs,gcexcel,forguncy,wyn
DOC_LANG=en

# === Server ===
PORT=8900
HOST=0.0.0.0

# === Voyage AI ===
VOYAGE_API_KEY=your_voyage_api_key_here
VOYAGE_EMBED_MODEL=voyage-code-3
VOYAGE_RERANK_MODEL=rerank-2.5
VOYAGE_RPM_LIMIT=2000
VOYAGE_TPM_LIMIT=3000000

# === Qdrant ===
# docker compose 环境下自动指向 qdrant 容器，本地开发填 localhost
QDRANT_URL=http://localhost:6333
# QDRANT_API_KEY=

# === Embedding ===
CHUNK_SIZE=3000
BATCH_SIZE=128

# === Logging ===
LOG_LEVEL=info
